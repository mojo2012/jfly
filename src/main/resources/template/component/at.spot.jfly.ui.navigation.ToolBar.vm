<v-toolbar uuid="$!{component.uuid()}" class="navbar $!{component.getCssStyleString()}" $!events
	v-model="componentStates['${component.uuid()}']" app absolute clipped-left
	>

	$!{component.getLeftActionItem().render()}
	
	<v-toolbar-title>$!{component.getHeader().text()}</v-toolbar-title>
	
	#foreach( $!child in $!component.children() )
		$!child.render()
	#end
    
    $!{component.getRightActionItem().render()}
</v-toolbar>