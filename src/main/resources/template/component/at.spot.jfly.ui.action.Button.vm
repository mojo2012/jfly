<v-btn type="$!{component.componentType().internalName()}" uuid="$!{component.uuid()}" 
	class="$!{component.getCssStyleString()}" $!events
	v-model="componentStates['${component.uuid()}']" color="info" :flat="componentStates.${component.uuid()}.isFlat" 
	#if ( ${component.getIcon()} && !${component.text()} ) icon #end
	>
	
	$!{component.getIcon().render()}
	
	{{componentStates.$!{component.uuid()}.text}}
	
</v-btn>