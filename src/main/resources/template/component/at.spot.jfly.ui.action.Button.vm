<v-btn uuid="$!{component.uuid}" 
	class="$!{component.cssStyleString}" $!events
	v-model="componentStates['${component.uuid}']" color="info" :flat="componentStates.${component.uuid}.isFlat" 
	#if ( ${component.icon} && !${component.text} ) icon #end
	>
	
	$!{component.icon.render()}
	
	{{componentStates.$!{component.uuid}.text}}
	
</v-btn>