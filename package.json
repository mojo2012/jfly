{
  "name": "jfly",
  "version": "1.0.0",
  "description": "modern vuetifyjs-based java web UI toolkit",
  "main": "index.js",
  "dependencies": {
    "jquery": "^3.3.1",
    "material-design-icons-iconfont": "^3.0.3",
    "vue": "^2.5.22",
    "vuetify": "^1.4.3"
  },
  "devDependencies": {
    "node-minify": "^2.4.2",
    "concat": "^1.0.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "concat:js": "mkdir -p target/classes/web/script && concat -o target/classes/web/script/deps.js node_modules/jquery/dist/jquery.min.js node_modules/vue/dist/vue.min.js node_modules/vuetify/dist/vuetify.min.js",
    "concat:css": "mkdir -p target/classes/web/style && concat -o target/classes/web/style/deps.css node_modules/vuetify/dist/vuetify.min.css node_modules/material-design-icons-iconfont/dist/fonts/material-icons.css",
    "concat": "npm run concat:css && npm run concat:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojo2012/jfly.git"
  },
  "keywords": [
    "vue",
    "vuetify",
    "java",
    "web",
    "ui-toolkit"
  ],
  "author": "Matthias Fuchs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mojo2012/jfly/issues"
  },
  "homepage": "https://github.com/mojo2012/jfly#readme"
}
